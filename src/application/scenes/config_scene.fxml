<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox fx:id="mainHBox" prefHeight="550.0" prefWidth="220.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ui.ConfigController">
   <children>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="550.0" prefWidth="215.0" style="-fx-background-color: #dddddd;">
         <children>
            <HBox>
               <children>
                  <ImageView fx:id="chatImageView" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </HBox.margin>
                  </ImageView>
                  <Label fx:id="chatLabel">
                     <HBox.margin>
                        <Insets bottom="5.0" right="5.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox>
               <children>
                  <TextField fx:id="add_tf" alignment="CENTER" prefHeight="25.0" prefWidth="175.0" promptText="room name">
                     <font>
                        <Font size="11.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="3.0" left="5.0" right="5.0" top="3.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="add_btn" maxHeight="25.0" maxWidth="25.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0">
                     <HBox.margin>
                        <Insets bottom="3.0" right="5.0" top="3.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <ScrollPane fx:id="contactsScrollPane" fitToWidth="true" prefViewportHeight="600.0" prefViewportWidth="460.0">
               <content>
                  <VBox fx:id="vboxOnScroll" />
               </content>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </ScrollPane>
         </children>
      </VBox>
      <Separator maxWidth="550.0" orientation="VERTICAL" prefHeight="5.0" />
   </children>
</HBox>
